{
  "name": "hydration-extension",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "yarn build:background && yarn build:content && yarn build:inject && yarn build:ui",
    "build:background": "tsc -b && vite build --config vite.config.background.ts",
    "build:content": "tsc -b && vite build --config vite.config.content.ts",
    "build:inject": "tsc -b && vite build --config vite.config.inject.ts",
    "build:ui": "tsc -b && vite build --config vite.config.ui.ts",
    "lint": "eslint .",
    "preview": "vite preview",
    "zip": "zip -r hydration-extension-v${npm_package_version}.zip dist"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.10.1",
    "diff": "^8.0.2",
    "js-beautify": "^1.15.4",
    "next": "^16.0.3",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/chrome": "^0.1.28",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/uuid": "^11.0.0",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
